<div class="well span2 quick_dash_container">
  <div class="quick_dash_title">Current Weather</div>
  <div class="row">
    <ul class="unstyled">
      <div class="span1">
        <li id="weather_icon"><%= image_tag(as_photo(@weather.icon)) %></li>
      </div>
      <div class="span1 no_margin" style="width:100px">
        <li>Temp: <%= @weather.temperature %> F</li>
        <li><%= @weather.humidity %></li>
        <li><%= @weather.condition %></li>
      </div>
    </ul>
  </div>
</div>

<div class="well span2 quick_dash_container">
  <div class="quick_dash_title">Walk Score</div>
  <ul class="unstyled">
    <div class="walkscore">
      <li><%= @city.walk_score.score %></li>
    </div>
  </ul>
  <div class="bottom_caption"><%= @city.walk_score.description %></div>
</div>
<div class="clearfix"></div>