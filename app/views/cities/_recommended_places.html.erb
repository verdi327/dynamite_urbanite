<div class="well">
  <div class="page-header"><h3>Where the locals hang<h3></div>
  <div class="row">
    <div class="span3">
      <ul class="unstyled">
      <% @city.foursquare_places[0..9].each do |item| %>
        <li class="foursquare"><%= image_tag(item.icon) %> - <%= item.venue_name %></li>
      <% end %>
      </ul>
    </div>
    <div class="span3 offset1">
      <ul class="unstyled">
      <% @city.foursquare_places[10..19].each do |item| %>
        <li class="foursquare"><%= image_tag(item.icon) %> - <%= item.venue_name %></li>
      <% end %>
      </ul>
    </div>
    <div class="span3 offset1">
      <ul class="unstyled">
      <% @city.foursquare_places[20..30].each do |item| %>
        <li class="foursquare"><%= image_tag(item.icon) %> - <%= item.venue_name %></li>
      <% end %>
      </ul>
    </div>
  </div>
</div>